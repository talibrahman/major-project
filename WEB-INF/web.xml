<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>FRoDo</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>upload</servlet-name>
    <servlet-class>pack.upload</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>upload</servlet-name>
    <url-pattern>/upload</url-pattern>
  </servlet-mapping>

   <servlet>
    <servlet-name>CryptoCurrency</servlet-name>
    <servlet-class>pack.CryptoCurrency</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CryptoCurrency</servlet-name>
    <url-pattern>/CryptoCurrency</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>Deposits</servlet-name>
    <servlet-class>pack.Deposits</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Deposits</servlet-name>
    <url-pattern>/Deposits</url-pattern>
  </servlet-mapping>

</web-app>